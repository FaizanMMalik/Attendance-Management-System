@model  IEnumerable<proj2mvc.Models.Functions>
@{
    ViewBag.Title = "Contacts";
}
<html>
<h2>Contacts</h2>
<body>
    <table class="table" border="1">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.mName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.newmsg)
            </th>
          


        </tr>
        @if (Model != null && Model.Count() > 0)
        {

            foreach (var item in Model)
            {
                using (Html.BeginForm("DeleteM", "Abs", new
                {
                    id = item.MID
                }))
                {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.mName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.newmsg)
        </td>

        <td>
         
            <input type="submit" value="Delete" class="btn btn-danger" onclick="return
                                confirm('Are you sure you want to delete this record?');" />
        </td>

    </tr>

                }
            }
        }
        else
        {
            <tr>
                <td colspan="10">
                    No Record Found
                </td>
            </tr>
        }
    </table>
</body>
</html>